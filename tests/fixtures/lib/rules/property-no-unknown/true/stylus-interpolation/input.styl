vendor(prop, args)
  -webkit-{prop} args
  -moz-{prop} args
  {prop} args

border-radius()
  vendor('border-radius', arguments)

box-shadow()
  vendor('box-shadow', arguments)

button
  border-radius 1px 2px / 3px 4px

table
  for row in 1 2 3 4 5
    tr:nth-child({row})
      height: 10px * row

mySelectors = '#foo,#bar,.baz'

{mySelectors}
  background: #000
