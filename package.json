{
    "name": "stylelint-plugin-stylus",
    "version": "0.11.0",
    "description": "Stylelint plugin for Stylus",
    "main": "lib/index.js",
    "files": [
        "lib",
        "custom-syntax",
        "standard",
        "recommended",
        "base-config",
        "no-stylus"
    ],
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test:base": "mocha \"tests/runs/**/*.js\" --reporter dot --timeout 60000",
        "test": "nyc npm run test:base -- --timeout 60000",
        "debug": "mocha \"tests/runs/**/*.js\" --reporter dot --timeout 60000",
        "cover:report": "nyc report --reporter=html",
        "test:cover": "npm run test && npm run cover:report",
        "update-fixtures": "cross-env UPDATE_FIXTURES=true npm run test:base",
        "preversion": "npm run update && npm run test",
        "version": "npm run lint -- --fix && git add .",
        "update": "node ./scripts/update.js",
        "docs:watch": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs-deploysetup": "npm run docs:build && npm run docs-deploysetup:clean && npm run docs-deploysetup:copy",
        "docs-deploysetup:clean": "rimraf assets",
        "docs-deploysetup:copy": "npx cpx \"docs/\\.vuepress/dist/**\" . -u",
        "stylelint": "stylelint \"docs/.vuepress/**/*.css\" \"docs/.vuepress/**/*.vue\" \"docs/.vuepress/**/*.styl\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ota-meshi/stylelint-plugin-stylus.git"
    },
    "keywords": [
        "stylelint-plugin",
        "stylelint",
        "stylus",
        "lint",
        "linter",
        "pretty",
        "beautify"
    ],
    "author": "Yosuke Ota",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ota-meshi/stylelint-plugin-stylus/issues"
    },
    "homepage": "https://ota-meshi.github.io/stylelint-plugin-stylus/",
    "peerDependencies": {
        "stylelint": "^13.2.1 || ^14.0.0",
        "postcss-syntax": "^0.36.2"
    },
    "peerDependenciesMeta": {
        "postcss-syntax": {
            "optional": true
        }
    },
    "devDependencies": {
        "@ota-meshi/eslint-plugin": "^0.10.0",
        "@types/lodash": "^4.14.149",
        "cross-env": "^7.0.2",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-json-schema-validator": "^1.0.0",
        "eslint-plugin-jsonc": "^1.0.0",
        "eslint-plugin-markdown": "^2.0.0-0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-regexp": "^1.0.0",
        "eslint-plugin-yml": "^0.10.0",
        "mocha": "^9.0.0",
        "nyc": "^15.0.1",
        "pako": "^2.0.4",
        "postcss": "^8.0.0",
        "postcss-scss": "^4.0.1",
        "postcss-syntax": "^0.36.2",
        "prettier": "^2.2.1",
        "stylelint": "^14.0.0",
        "stylelint-config-standard": "^23.0.0",
        "stylelint-plugin-stylus": "file:.",
        "stylelint4b": "^14.0.0-4b.0",
        "vue-stylelint-editor": "^0.5.0",
        "vuepress": "^1.4.0"
    },
    "dependencies": {
        "html-tags": "^3.1.0",
        "lodash": "^4.17.15",
        "mathml-tag-names": "^2.1.3",
        "postcss-media-query-parser": "^0.2.3",
        "postcss-selector-parser": "^6.0.2",
        "postcss-styl": "^0.9.0",
        "style-search": "^0.1.0",
        "stylelint-config-html": "^0.0.2 || ^1.0.0",
        "svg-tags": "^1.0.0"
    },
    "engines": {
        "node": "^12 || >=14"
    }
}
